doc = FreeCAD.newDocument("Test")
circle = Part.makeCircle(6, FreeCAD.Vector(0,0,0), FreeCAD.Vector(0,0,1))
wire = Part.Wire([circle])
face = Part.Face(wire)
cylinder = face.extrude(FreeCAD.Vector(0,0,80))
obj = doc.addObject("Part::Feature", "TestCylinder")
obj.Shape = cylinder
FreeCADGui.SendMsgToActiveView("ViewFit")
