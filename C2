gear = FreeCAD.ActiveDocument.InvoluteGear  # یا نام دقیق شیء شما


# پارامترهای کلیدی (مطابق نیاز شما)
gear.NumberOfTeeth = 20       # تعداد دندانه‌ها = ۲۰
gear.Module = 1.0             # مدول = ۱.۰ (مطابق چرخ‌دنده شانه‌ای)
gear.Height = 12.0            # ضخامت = ۱۲mm (مطابق چرخ‌دنده شانه‌ای)
gear.PressureAngle = 20.0     # زاویه فشار = ۲۰ درجه (استاندارد)

# اعمال تغییرات
gear.recompute()
